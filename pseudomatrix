function matrixmultipication (matriks_a, matriks_b)
  var hasil as array
    for int i from 0 to sizeof(matriks_a)
      for int j from 0 to sizeof(matriks_b[0])
      int temp = 0
      for int k from 0 to sizeof(matriks_b)
        temp = multiplication of matrix element
      hasil [i][j] = temp
   return hasil
